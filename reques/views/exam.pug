  <body ontouchstart>
    div
        textarea(id="items",style="display:none;")= JSON.stringify(rows)
        input(id="page",type="hidden",value=page)
        input(id="id",type="hidden")
    <p id="bear"></p>
    <div id="form">
        <div>
            h2= message
            <h3 id='title'>6. 下面程序的运行结果：（）</h3>
            <div class="page__code" id='code' style="display:none;">
            <pre>
            <span>public static void main(String args[]) {</span>
            <span>   Thread t = new Thread() {</span>
            <span>    public void run() {</span>
            <span>            pong();</span>
            <span>        }</span>
            <span>    };</span>
            <span>    t.run();</span>
            <span>    System.out.print("ping");</span>
            <span>}</span>
            <span>static void pong() {</span>
            <span>    System.out.print("pong");</span>
            <span>}</span>
            </pre>
            </div>
        </div>


        <div class="weui-cells__title">请选择(1-2个)</div>
        <div id='choHeader' class="weui-cells weui-cells_checkbox">
            <label class="weui-cell weui-check__label" for="c1">
                <div class="weui-cell__hd">
                    <input type="checkbox" tips="请勾选1-2个答案" class="weui-check" name="assistance" id="c1">
                    <i class="weui-icon-checked"></i>
                </div>
                <div class="weui-cell__bd" id='A'>A. pingpong</div>
            </label>
            <label class="weui-cell weui-check__label" for="c2">
                <div class="weui-cell__hd">
                    <input type="checkbox" name="assistance" class="weui-check" id="c2">
                    <i class="weui-icon-checked"></i>
                </div>
                <div class="weui-cell__bd" id='B'>B. pongping</div>
            </label>
            <label class="weui-cell weui-check__label" for="c3">
                <div class="weui-cell__hd">
                    <input type="checkbox" name="assistance" class="weui-check" id="c3">
                    <i class="weui-icon-checked"></i>
                </div>
                <div class="weui-cell__bd" id='C'>C. pingpong和pongping都有可能 </div>
            </label>
            <label class="weui-cell weui-check__label" for="c4">
                <div class="weui-cell__hd">
                    <input type="checkbox" name="assistance" class="weui-check" id="c4">
                    <i class="weui-icon-checked"></i>
                </div>
                <div class="weui-cell__bd" id='D'>D. 都不输出</div>
            </label>
        </div>
        <div class="weui-btn-area">
            <a id="prev" href="javascript:prev()" class="weui-btn weui-btn_mini weui-btn_primary weui-btn_plain-default">上一个</a>
            <a id="next" href="javascript:next(this)" class="weui-btn weui-btn_mini weui-btn_primary weui-btn_primary">下一个</a>
        </div>
</div>

